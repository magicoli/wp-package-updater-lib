{
  "name": "magicoli/wp-package-updater-lib",
  "description": "Composer package for wp-package-updater library.",
  "type": "library",
  "license": "AGPL-3.0-or-later",
  "autoload": {
    "psr-4": {
      "Magicoli\\WpPackageUpdaterLib\\": "lib/wp-package-updater-lib"
    }
  },
  "authors": [
    {
      "name": "Magiiic"
    }
  ],
  "repositories":[
    {
      "type": "package",
      "package": {
        "name": "froger-me/wp-plugin-update-server",
        "version": "1.4.15",
        "source": {
          "url": "https://github.com/froger-me/wp-plugin-update-server",
          "type": "git",
          "reference": "master"
        }
      }
    }
  ],
  "scripts": {
    "post-package-install": [
      "echo RUNNING post-package-install",
      "php installer.php"
    ],
    "post-package-upate": [
      "echo RUNNING post-package-update",
      "php installer.php"
    ],
    "post-update-cmd": [
      "echo RUNNING post-update-cmd",
      "rsync --delete -Wavz vendor/froger-me/wp-plugin-update-server/wp-plugin-update-server/integration-examples/dummy-plugin/lib/plugin-update-checker/ lib/plugin-update-checker/",
      "rsync --delete -Wavz vendor/froger-me/wp-plugin-update-server/wp-plugin-update-server/integration-examples/dummy-plugin/lib/wp-package-updater/ lib/wp-package-updater/"
    ]
  },
  "require-dev": {
    "froger-me/wp-plugin-update-server": "*"
  }
}
